{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "dynamicImport": false,
      "decorators": false
    },
    "transform": {
      "react": {
        "runtime": "automatic",
        "useBuiltins": true,
        "refresh": true
      }
    },
    "target": "esnext",
    "loose": false,
    "externalHelpers": false,
    // Requires v1.2.50 or upper and requires target to be es2016 or upper.
    "keepClassNames": false
  },
  "module": {
    "type": "es6"
  },
  "exclude": [".*\\.test.ts$", ".*\\.test.tsx$"],
  "minify": false
}
